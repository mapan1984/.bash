#!/usr/bin/python3
# encoding: utf-8

"""输不需要同步的bash配置文件"""

import os

# 需要同步的配置文件
config_file='.bash_profile .bashrc .bash_logout bin .gitconfig .gitignore'
config_file=config_file.split(" ")

ignore = []
for i in os.listdir('/home/mapan/'):
    if i not in config_file:
        ignore.append(i)

for i in ignore:
    print(i)
